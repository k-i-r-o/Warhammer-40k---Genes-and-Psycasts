  <!-- Make geneseed base -->

  <BEWH_AstartesPack.label>Make astartes genepack</BEWH_AstartesPack.label>
  <BEWH_AstartesPack.description>Remove fully grown germ cells from progenoid glands to make all gene-seeds of an astartes.</BEWH_AstartesPack.description>
  <BEWH_AstartesPack.jobString>creating gene-seed from progenoid glands</BEWH_AstartesPack.jobString>
  <BEWH_AstartesPack.successfullyRemovedHediffMessage>{0} has successfully created gene-seed from {1}'s progenoid glands.</BEWH_AstartesPack.successfullyRemovedHediffMessage>


  <!-- Make primaris geneseed base -->

  <BEWH_PrimarisPack.label>Make primaris genepack</BEWH_PrimarisPack.label>
  <BEWH_PrimarisPack.description>Remove fully grown germ cells from progenoid glands to make all gene-seeds of a primaris.</BEWH_PrimarisPack.description>
